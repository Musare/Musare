    "cors": "^2.8.5",
    "express": "^4.18.2",
    "moment": "^2.29.4",
    "mongoose": "^6.12.6",
    "nodemailer": "^6.9.2",
    "oauth": "^0.10.0",
    "redis": "^4.6.6",
    "retry-axios": "^3.0.0",
    "sha256": "^0.2.0",
