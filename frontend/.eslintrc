{
	"root": true,
	"env": {
		"browser": true,
		"amd": true,
		"node": true,
		"es6": true
	},
	"parser": "vue-eslint-parser",
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module",
		"requireConfigFile": false
	},
	"extends": [
		"airbnb-base",
		"plugin:vue/strongly-recommended",
		"eslint:recommended",
		"prettier"
	],
	"plugins": [
		"prettier"
	],
	"globals": {
		"lofig": "writable",
		"grecaptcha": "readonly",
		"history": "readonly"
	},
	"rules": {
		"no-console": 0,
		"no-control-regex": 0,
		"no-var": 2,
		"no-underscore-dangle": 0,
		"radix": 0,
		"no-multi-assign": 0,
		"no-shadow": 0,
		"no-new": 0,
		"import/no-unresolved": 0,
		"import/extensions": 0,
		"prettier/prettier": [
			"error"
		],
		"vue/order-in-components": 2,
		"vue/no-v-for-template-key": 0,
		"vue/multi-word-component-names": 0
	}
}